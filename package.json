    "jspdf": "^3.0.4",
    "lucide-react": "^0.344.0",
    "mysql2": "^3.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "pdfjs-dist": "^5.4.296",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.8.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.6.0",
