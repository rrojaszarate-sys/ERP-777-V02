  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-signature-canvas": "^1.0.7",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^15.6.0",
    "cypress-mochawesome-reporter": "^3.8.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.9.1",
