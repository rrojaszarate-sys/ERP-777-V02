{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "ocr:backend": "cd server && npm start",
    "ocr:install": "cd server && npm install",
    "cargar:datos": "node scripts/cargar_datos_v2.mjs",
    "limpiar:eventos": "node scripts/limpiar_crear_eventos_prueba.mjs",
    "gni:cargar-catalogos": "node scripts/cargar_catalogos_gni.mjs",
    "gni:validar": "node scripts/validar_consistencia_gni.mjs",
    "test:gni": "cypress run --spec 'cypress/e2e/11-gastos-no-impactados.cy.js'",
    "crear:cuentas": "node scripts/crear_cuentas_base.mjs",
    "restaurar:base": "node scripts/restaurar_datos_base.mjs",
    "restaurar:backup": "node scripts/restaurar_desde_backup.mjs",
    "setup:pruebas": "./scripts/setup_pruebas_completo.sh",
    "test:automatizado": "node scripts/ejecutar_pruebas_automatizadas.mjs",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:ui": "cypress run --browser chrome",
    "test:ui:headed": "cypress run --browser chrome --headed",
    "test:ui:report": "cypress run --browser chrome && echo 'Reporte generado en cypress/reports/'",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:report": "npx playwright show-report playwright/reports",
    "migrate": "node scripts/migrate.mjs",
    "backup": "bash scripts/backup-db.sh",
    "docs:generate": "bash scripts/generar-documentacion.sh",
    "docs:obsoletos": "node scripts/identificar-obsoletos.mjs",
    "cleanup": "bash scripts/mover-obsoletos.sh",
    "deploy": "bash scripts/deploy.sh",
    "publish": "bash scripts/commit-and-publish.sh",
    "deploy:full": "bash EJECUTAR_TODO_AHORA.sh",
    "git:commit": "bash commit-y-publicar.sh"
  },
