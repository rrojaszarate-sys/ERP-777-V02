# Configuracion de Supabase
# Copia este archivo como .env y completa con tus credenciales

# URL del proyecto Supabase
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co

# Clave publica (anon key) para el cliente
VITE_SUPABASE_ANON_KEY=tu_anon_key_aqui

# Clave de servicio (service role) para scripts de administracion
# IMPORTANTE: Esta clave tiene acceso total, no exponerla en el frontend
VITE_SUPABASE_SERVICE_ROLE_KEY=tu_service_role_key_aqui

# Configuracion opcional
VITE_APP_NAME=ERP-777-V02
VITE_APP_VERSION=2.0.0

# Configuracion fiscal Mexico
VITE_IVA_PORCENTAJE=16
